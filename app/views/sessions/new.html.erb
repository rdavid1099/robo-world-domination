<center>
  <%= render partial: "shared/flash" %>
  <h4>Log In to Robo World Domination</h4>
  <%= form_for(:sessions) do |f| %>
    <%= f.text_field :username, placeholder: "Username" %><br><br>
    <%= f.password_field :password, placeholder: "Password" %><br><br>

    <%= f.submit "Log In" %>
  <% end %>
</center>
